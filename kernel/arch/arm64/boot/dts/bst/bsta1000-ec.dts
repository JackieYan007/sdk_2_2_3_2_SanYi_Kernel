        spi-max-frequency = <4000000>;
        reg = <0>;
        mode = <0>;
            
        partition@0 {
            reg = <0 0x1e00000>;
            label = "nor1_part0";
        };
        partition@1e00000 {
            reg = <0x1e00000 0x200000>;
            label = "nor1_part1";
        };
    };
};


