/*
  imx390-raw    1920x1080
  view0:        1920x1080
  view1:        1280x720
  view2:        320x180
*/
compatible = "bst,imx390";
type = "imx390";
sensor-id = <0x21>;
data-type = <0x2c>;
fv-polarity-low = <0>;
fpd3-mode = "csi-2";
serializer = "ti953";
algo-online = "imx390online/imx390_raw12_sy_h60_online_AlgoParam.bin";
iq-online = "imx390online/imx390_raw12_sy_h60_IqParam.bin";

hdr-stagger-en = <1>;
exp-num = <3>;
size = <1920 1084>;
dvp-dummy = <0xaaa0>;

view0-fmt = <1>;
view0-size = <1280 720>;

view1-fmt = <1>;
view1-size = <1920 1080>;

view2-fmt = <0>;
view2-size = <320 180>;

pdns-mode = <0>;
pdns-input-view = <0>;

hblank = <0x800>;

isp-top-crop = <2>;
isp-bot-crop = <2>;
