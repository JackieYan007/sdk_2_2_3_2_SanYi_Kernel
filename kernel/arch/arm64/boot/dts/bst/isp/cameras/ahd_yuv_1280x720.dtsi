/*
  ahd-yuv   1920x1080
  view0:    1920x1080
  view1:    -
  view2:    320x180
*/
compatible = "bst,ahd-isp";
data-type = <0x1e>;

size = <1280 720>;
dvp-dummy = <0xaaa0>;

view0-fmt = <1>;
view0-size = <1280 720>;

view2-fmt = <0>;
view2-size = <320 180>;

pdns-mode = <0>;
pdns-input-view = <0>;
hblank = <0x800>;
